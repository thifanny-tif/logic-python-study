#dados os valores de horario abaixo, decifre a lógica e faça um codigo para executar
# entrada 1: 3:15
# entrada 2: 14:20
# saída 6:05

entrada1 = "3:15"
entrada2 = "14:20"

h1, m1 = map(int, entrada1.split(":"))
h2, m2 = map(int, entrada2.split(":"))

totalHo = h1 + h2
totalMi = m1 + m2

if totalMi >= 60:
    totalHo += totalMi // 60
    totalMi = totalMi % 60

saida = f"{totalHo}:{totalMi:02}"

print("saida:", saida)
